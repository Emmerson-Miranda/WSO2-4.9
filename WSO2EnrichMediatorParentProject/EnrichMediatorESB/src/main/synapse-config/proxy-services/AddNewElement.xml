<?xml version="1.0" encoding="UTF-8"?>
<proxy name="AddNewElement" startOnLoad="true" trace="disable"
  transports="http https" xmlns="http://ws.apache.org/ns/synapse">
  <target>
    <inSequence>
      <property description="myCustomProperty" name="MY_VALUE"
        scope="default" type="STRING" value="this is a custom value"/>
      <enrich>
        <source clone="true" type="inline">
          <newElement xmlns="">1234</newElement>
        </source>
        <target action="child"
          xmlns:echo="http://echo.services.core.carbon.wso2.org" xpath="//container"/>
      </enrich>
      <respond/>
    </inSequence>
    <outSequence/>
    <faultSequence/>
  </target>
</proxy>
